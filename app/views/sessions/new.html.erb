<div class="jumbotron">
<div class="container">
  <div class="login-inner-container">


<h1>Login</h1>

<%= render partial: 'shared/error_messages', locals: { object: @user } %>
<div class="form-horizontal" >
  <%= form_for :login do |f| %>

    <div class="col-md-10 form-group input-group">
    <%= f.label :username %>
    <%= f.text_field :username, class: 'form-control' %>
    </div>

    <div class="col-md-10 form-group input-group">
    <%= f.label :password %>
    <%= f.password_field :password, class: 'form-control' %>
    </div><br>

    <%= f.submit "Login", class: 'btn btn-default' %>

  <% end %>
</div>
</div>
</div>
</div>