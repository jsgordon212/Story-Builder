  <%= render partial: 'shared/error_messages', locals: { object: @character } %>



<%= form_for (@character) do |f| %>

  <p>
    <%= f.label :name %>
    <%= f.text_field :name %>

  </p>
  <p>
    <%= f.label :description %>
    <%= f.text_field :description %>
  </p>

  <p>
    <!-- dropdown -->
  </p>

    <%= f.hidden_field :project_id, :value => @project.id %>
  <p>
    <%= f.submit "Submit" %>
  </p>

<% end %>