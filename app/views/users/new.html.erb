<div class="jumbotron">
  <div class="container">
    <div class="register-inner-container">

<h1>Register</h1>

<%= render partial: 'shared/error_messages', locals: { object: @user } %>

<%= form_for :user, url: { action: "create" } do |f| %>

  <div class="row-xs-3 form-group input-group">
  <%= f.label :username %>
  <%= f.text_field :username, class: 'form-control' %>
  </div>

  <div class="row-xs-3 form-group input-group">
  <%= f.label :email %>
  <%= f.text_field :email, class: 'form-control' %>
  </div>

  <div class="row-xs-3 form-group input-group">
  <%= f.label :password %>
  <%= f.password_field :password, class: 'form-control' %>
  </div><br>

  <%= f.submit "Register", class: 'btn btn-default' %>

<% end %>
</div>
</div>
</div>
