<%= form_for @project do |f| %>
  <%= render partial: 'shared/error_messages', locals: { object: @project } %>
  <p>
    <%= f.label :title %><br>
    <%= f.text_field :title %>
  </p>
  <p>
    <%= f.label :summary %><br>
    <%= f.text_area :summary, cols: "50", rows: "10" %>
  </p>
    <%= hidden_field(:project, :user_id) %>
  <p>
    <%= f.submit %>
  </p>

<% end %>
